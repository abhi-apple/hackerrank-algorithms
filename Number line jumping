#include <iostream>
using namespace std;
int main()
{
    int x1;
    int v1;
    int x2;
    int v2;
    cin >> x1 >> v1 >> x2 >> v2;
    int k;
    for (int i = 0; i < 10000; i++)
    {
        if ((x1 + i * v1) == (x2 + i * v2))
        {
            cout << "YES";
            k=1;
            return 0;
        }
    }
    if(k!=1){
        cout<<"NO";
    }
    return 0;
}
