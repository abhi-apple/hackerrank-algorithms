#include<iostream>
using namespace std;
void cl(){
    int n;
    int k;
    cin>>n;
    cin>>k;
    int arr[100000];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int count=0;
    for(int i=0;i<n;i++){
        if(arr[i]<=0){
            count++;
        }
    }
    if(count>=k){
        cout<<"NO";
    }else{
        cout<<"YES";
    }
}
int main(){
    int t;
    cin>>t;
    for(int i=0;i<t;i++){
        cl();
        cout<<endl;
    }

}
